<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <TextView
        android:id="@+id/tv_idCategory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="idCategory"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv_strCategory"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="strCategory"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv_strCategoryThumb"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="strCategoryThumb"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/tv_strCategoryDescription"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="strCategoryDescription"
        android:textSize="20dp" />


</LinearLayout>